{
  "name": "influxclient",
  "version": "0.0.1",
  "title": "Influx",
  "type": "flogo:activity",
  "description": "Writes the message object to influx db",
  "ref": "github.com/AnnapoorniS/flogoSample/activity/influxclient",
  "author": "Your Name <you.name@example.org>",
  "inputs":[
    {
      "name": "data",
      "type": "string"
    },
    {
      "name": "tagkey",
      "type": "string"
    },
    {
      "name": "tagvalue",
      "type": "string"
    },
    {
      "name": "measurement",
      "type": "string",
      "required": true
    },
    {
      "name": "addr",
      "type": "string",
      "required": true
    },
    {
      "name": "database",
      "type": "string",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "output",
      "type": "string"
    },
    {
      "name": "msg_data",
      "type": "any"
    }
  ]
}
